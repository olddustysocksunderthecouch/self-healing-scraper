{
  "name": "self-healing-scraper",
  "version": "0.1.0",
  "description": "A self-healing web scraper that can detect and repair itself when target websites change.",
  "type": "module",
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "devDependencies": {
    "@eslint/js": "~9.17",
    "@types/eslint__js": "~8.42",
    "@types/jest": "^29.5.12",
    "@types/node": "~20",
    "@typescript-eslint/parser": "~8.19",
    "@vitest/coverage-v8": "~2.1",
    "@vitest/eslint-plugin": "~1.1",
    "eslint-config-prettier": "~9.1",
    "eslint": "~9.17",
    "globals": "~15.14",
    "jest": "^29.7.0",
    "jest-puppeteer": "^9.0.0",
    "expect-puppeteer": "^7.0.0",
    "prettier": "~3.4",
    "rimraf": "~6.0",
    "ts-api-utils": "~2.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript-eslint": "~8.19",
    "typescript": "~5.7",
    "vitest": "~2.1"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:all": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "prettier": "prettier \"{src,tests}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,tests}/**/*.{ts,mts}\" --config .prettierrc --check",
    "scrape": "node --loader ts-node/esm src/cli/selfheal.ts scrape"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "puppeteer": "^22.10.0",
    "tslib": "~2.8"
  },
  "volta": {
    "node": "22.12.0"
  }
}
