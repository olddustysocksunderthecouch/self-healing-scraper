{
  "name": "self-healing-scraper",
  "version": "0.1.0",
  "description": "A robust web scraping framework with built-in self-healing capabilities powered by AI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/cli/selfheal.ts",
    "scrape": "ts-node src/cli/selfheal.ts scrape",
    "scrape:pretty": "ts-node src/cli/selfheal.ts scrape --pretty",
    "test": "jest",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "web-scraping",
    "self-healing",
    "ai",
    "automation"
  ],
  "author": "Self-Healing Scraper Contributors",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/self-healing-scraper.git"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.3",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.8.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
